/**
 * Check if a school name is a DO school
 * @param {string} schoolName - School name to check
 * @returns {boolean} True if DO school, false otherwise
 */
export const isDOSchool = (schoolName) => {
  if (!schoolName) return false
  const normalized = schoolName.toLowerCase().trim()

  // Check full name match
  const fullNameMatch = doSchools.some(school =>
    school.name.toLowerCase() === normalized
  )

  // Check shorthand match
  const shorthandMatch = !!doSchoolShorthands[normalized]

  return fullNameMatch || shorthandMatch
}

/**
 * Get full DO school name from shorthand
 * @param {string} shorthand - Shorthand abbreviation
 * @returns {string|null} Full school name or null
 */
export const getDOSchoolFullName = (shorthand) => {
  if (!shorthand) return null
  return doSchoolShorthands[shorthand.toLowerCase().trim()] || null
}
