// Mapping of undergraduate institution names to their medical school counterparts
export const undergraduateToMedicalSchoolMap = {
  'Northwestern University': 'Northwestern University Chicago',
  'Columbia University in the City of New York': 'Columbia University Health Sciences',
  'New York University': 'New York University School of Medicine',
  'University of Pittsburgh Pittsburgh Campus': 'University of Pittsburgh',
  'University of Washington Seattle Campus': 'University of Washington Seattle',
  'University of Colorado Denver/Anschutz Medical Campus': 'University of Colorado Denver',
  'Cornell University': 'Cornell University Weill Medical College',
  'Case Western Reserve University': 'Case Western Reserve U & Cleveland Clinic Lerner Com',
  'University of Maryland College Park': 'University of Maryland Baltimore',
  'Indiana University Bloomington': 'Indiana University Indianapolis',
  'Ohio State University Main Campus': 'Ohio State University'
}

// Function to get NIH rank for a school
export const getNIHRank = (institutionName) => {
  // First try direct match
  let school = medicalSchools.find(school => school.name === institutionName)
  
  // If no direct match, check if there's a mapping
  if (!school && undergraduateToMedicalSchoolMap[institutionName]) {
    const mappedName = undergraduateToMedicalSchoolMap[institutionName]
    school = medicalSchools.find(school => school.name === mappedName)
  }
  
  return school ? school.rank : null
}

// Function to determine if school is Top 40 NIH
export const isTop40NIH = (institutionName) => {
  const rank = getNIHRank(institutionName)
  return rank !== null && rank <= 40
}
